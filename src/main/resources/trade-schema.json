            "$id": "#/properties/empAddress",
            "type": "object",
            "title": "The empAddress schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "streetName": "1st lane",
                    "LineName": "2nd Line",
                    "city": "city",
                    "country": "country",
                    "state": "state"
                }
            ],
            "required": [
                "LineName",
                "city",
                "country",
                "state",
                "streetName"
            ],
            "additionalProperties": true,
            "properties": {
                "LineName": {
                    "$id": "#/properties/empAddress/properties/LineName",
                    "type": "string",
